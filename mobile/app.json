{
  "expo": {
    "name": "CesiZen",
    "slug": "cesi-zen",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./src/assets/favicon.png",
    "splash": {
      "image": "./src/assets/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/favicon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png",
      "headTags": [
        { "tag": "link", "attributes": { "rel": "stylesheet", "href": "https://fonts.googleapis.com/icon?family=Material+Icons" }},
        { "tag": "link", "attributes": { "rel": "stylesheet", "href": "https://cdn.jsdelivr.net/npm/@expo/vector-icons@13.0.0/build/MaterialCommunityIcons.css" }}
      ]
    },
    "sdkVersion": "52.0.0",
    "newArchEnabled": true
  }
}
