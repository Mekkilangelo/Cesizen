{
  "expo": {
    "name": "CesiZen",
    "slug": "cesi-zen",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./src/assets/favicon.png",
    "splash": {
      "image": "./src/assets/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/favicon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "sdkVersion": "52.0.0",
    "newArchEnabled": true
  }
}
